      "source": [
        "import pandas as pd\n",
        "import plotly.express as px\n",
        "import matplotlib.pyplot as plt"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "df = pd.read_excel('/content/reshaped_dataset.xlsx')"
      ],
      "metadata": {
        "id": "TK6EkVZ441rY"
      },
      "execution_count": 2,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# ---- Creating 3 Buckets ----\n",
        "def bucket_inflation(x):\n",
        "    if x < 1:\n",
        "        return \"<1%\"\n",
        "    elif 1 <= x <= 3:\n",
        "        return \"1-3%\"\n",
        "    else:\n",
        "        return \">3%\"\n",
        "\n",
        "df['Bucket'] = df['Value'].apply(bucket_inflation)\n"
      ],
      "metadata": {
        "id": "iDtcgNRL479Y"
      },
      "execution_count": 3,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# ----Counting  categories per bucket per month ----\n",
        "dispersion = df.groupby(['Date', 'Bucket']).size().unstack(fill_value=0)"
      ],
      "metadata": {
        "id": "mdFnfJYM49z-"
      },
      "execution_count": 4,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# ---- Normalizing counts to percentages ----\n",
        "dispersion_percent = dispersion.div(dispersion.sum(axis=1), axis=0) * 100"
      ],
      "metadata": {
        "id": "mt1G5VPy4_kQ"
      },
      "execution_count": 5,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# ---- Counting  categories per bucket per month ----\n",
        "dispersion = df.groupby(['Date', 'Bucket'])['Category'].apply(list).reset_index()\n",
        "dispersion['Count'] = dispersion['Category'].apply(len)\n",
        "dispersion['Category_str'] = dispersion['Category'].apply(\n",
        "    lambda x: '<br>'.join(x)\n",
        ")\n"
      ],
      "metadata": {
        "id": "D6StLSN65GT5"
      },
      "execution_count": 6,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Defining the desired stack order (bottom -> top)\n",
        "bucket_order = [\"<1%\", \"1-3%\", \">3%\"]\n",
        "\n",
        "# Ensure Bucket column respects this order\n",
        "dispersion['Bucket'] = pd.Categorical(\n",
        "    dispersion['Bucket'],\n",
        "    categories=bucket_order,\n",
        "    ordered=True\n",
        ")\n",
        "\n",
        "# Sorting dataframe accordingly\n",
        "dispersion = dispersion.sort_values(['Date', 'Bucket'])\n",
        "\n",
        "fig = px.bar(\n",
        "    dispersion,\n",
        "    x='Date',\n",
        "    y='Count',\n",
        "    color='Bucket',\n",
        "    custom_data=['Category_str'],
        "    labels={'Count': 'Number of Categories', 'Date': 'Month'},\n",
        "    color_discrete_map={'<1%':'green','1-3%':'orange','>3%':'red'},\n",
        ")\n",
        "\n",
        "\n",
        "\n",
        "fig.update_traces(\n",
        "    hovertemplate=(\n",
        "        '<b>%{x}</b><br>'\n",
        "        'Bucket: %{fullData.name}<br>'\n",
        "        'Categories: %{customdata[0]}<br>'\n",
        "        'Count: %{y}'\n",
        "        '<extra></extra>'\n",
        "    )\n",
        ")\n",
        "\n",
        "fig.update_layout(\n",
        "    title='CPI Inflation Dispersion Over Time',\n",
        "    barmode='stack',\n",
        "    xaxis_tickangle=-45,\n",
        "    yaxis_title='Number of Categories',\n",
        "    hovermode='closest'\n",
        ")\n"
      ],
